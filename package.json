{
	"name": "rds",
	"version": "1.0.0",
	"description": "Never more",
	"repository": {
		"type": "git",
		"url": "git://github.com/ravendesignsystem/rds.git"
	},
	"scripts": {
		"mixdev": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
		"watch": "NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
		"hot": "NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
		"mixprod": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
		"sculpin": "vendor/bin/sculpin generate --watch --server",
		"sculpin-prod": "vendor/bin/sculpin generate --clean --server --env=prod",
		"dev": "concurrently --kill-others \"vendor/bin/sculpin generate --watch --server\" \"NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js\"",
		"prod": "concurrently \"vendor/bin/sculpin generate --clean --server --env=prod\" \"NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js\"  "
	},
	"prettier": {
		"useTabs": true,
		"printWidth": 80,
		"tabWidth": 1,
		"singleQuote": true,
		"trailingComma": "es5",
		"jsxBracketSameLine": false,
		"noSemi": true,
		"rcVerbose": true
	},
	"main": "index.js",
	"author": "",
	"license": "ISC",
	"dependencies": {
		"ajv": "^6.7.0",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-preset-env": "^1.7.0",
		"debug": "^4.1.1",
		"hoek": "^6.1.2 ",
		"imagemin": "^6.1.0"
	},
	"devDependencies": {
		"browser-sync": "^2.26.3",
		"browser-sync-webpack-plugin": "^2.0.1",
		"clean-webpack-plugin": "^1.0.1",
		"concurrently": "^4.1.0",
		"css-mqpacker": "^7.0.0",
		"cssnano": "^4.1.8",
		"laravel-mix": "^4.0.14",
		"node-sass": "^4.11.0",
		"postcss-pxtorem": "^4.0.1",
		"prettier": "^1.16.1",
		"resolve-url-loader": "^3.0.0",
		"sass": "^1.16.1",
		"sass-loader": "^7.1.0",
		"vue-template-compiler": "^2.5.22"
	}
}
