	"scripts": {
		"dev": "vendor/bin/sculpin generate --watch --server & NODE_ENV=development webpack --env=dev --mode=development --config webpack.dev.js --watch --colors",
		"prod": "vendor/bin/sculpin generate --clean --server --env=prod & NODE_ENV=production webpack --env=prod --mode=production --config webpack.prod.js --progress --hide-modules --colors",
		"release": "webpack --env=prod --mode=production --config webpack.release.js"
	},
