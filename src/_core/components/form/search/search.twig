<form class="c-form-search u-width-s" role="search" method="get" action="https://carleton.ca">
    <label for="search__input">Search</label>
    <input class="search__input" id="search__input" name="s" type="search"
           autocomplete="off"
           placeholder="Search">
    <button class="search__button u-icon u-icon--s" type="submit"
            title="Search">
        {% include "icons/search.svg" %}
        <span>Search button</span>
    </button>
</form>
